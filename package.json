{
	"name": "@awesome-typescript/aspect-ratio-calculator",
	"version": "1.0.1",
	"description": "@awesome-typescript/aspect-ratio-calculator",
	"author": "sander.vergeles",
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/awesome-typescript/aspect-ratio-calculator.git"
	},
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"test": "npm run eslint && npm run prettier:lint && npm run test:jest",
		"test:jest": "jest ./src --coverage",
		"lint": "npm run eslint && npm run prettier:lint",
		"eslint": "eslint ./src",
		"prettier": "prettier --write ./src/**/**.ts",
		"prettier:lint": "prettier --check ./src/**/**.ts",
		"clean": "rimraf dist",
